<h1>Lojas</h1>
<br>

<p><%= notice %></p>
<p><%= alert %></p>
<br>

<% @stores.each do |st| %>
  <p>Endereço: <%= st.adress%></p>
  <p>Telefone: <%= st.telephone %></p>
  <%= link_to "(Excluir loja)", destroy_store_path(id: st.id), method: :delete, data: { confirm: "Tem certeza que você deseja excluir?"} %>
  <br>
  
<% end %>
